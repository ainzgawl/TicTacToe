Board = [0, 1, 2,
         3, 4, 5,
         6, 7, 8]
def Visuel (Board):
    print ("Table de jeux : ", "\n")
    print (Board[0], "|", Board[1], "|", Board[2])
    print ("----------")
    print (Board[3], "|", Board[4], "|", Board[5])
    print ("----------")
    print (Board[6], "|", Board[7], "|", Board[8])
    print ("\n", "Choissisez o√π jouer : ")

Visuel(Board)

